<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ELearningServlet</servlet-name>
        <servlet-class>servlet.ELearningServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ELearningServlet</servlet-name>
        <url-pattern>/ELearningServlet</url-pattern>
        <url-pattern>/AddBookServlet</url-pattern>
        <url-pattern>/AddSyllabusServlet</url-pattern>
        <url-pattern>/AddCourseServlet</url-pattern>
        <url-pattern>/AddExamServlet</url-pattern>
        <url-pattern>/AddFeesServlet</url-pattern>
        <url-pattern>/AddStudentServlet</url-pattern>
        <url-pattern>/AddSubjectServlet</url-pattern>
         <url-pattern>/LoginServlet</url-pattern>
         <url-pattern>/FeedbackServlet</url-pattern>
          <url-pattern>/ShowSubjectServlet</url-pattern>
          <url-pattern>/ShowSyllabusServlet</url-pattern>
          <url-pattern>/ShowFeesServlet</url-pattern>
          <url-pattern>/ShowBookServlet</url-pattern>
          <url-pattern>/ShowCourseServlet</url-pattern>
          <url-pattern>/ShowExamServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
